<% layout('layout') -%>
<!-- products -->
<div class="products">
  <% if(data.length === 0) { %>
      <div class="container">
        <h3>No results found</h3>
      </div>
  <% } else { %>
  <div class="container" >
    <h3>Results for <%= query %></h3>
    <div class="col-md-12 product-w3ls-right">

      <div class="products-row">
        <% for(var i=0; i < data.length; i++) { %>
        <div class="col-md-3 product-grids">
          <div class="agile-products">

            <a href="/product/<%= data[i]._source._id %>"><img src="<%= data[i]._source.image %>" class="img-responsive" alt="img"></a>
            <div class="agile-product-text">
              <h5><a href="/product/<%= data[i]._source._id %>"><%= data[i]._source.name %></a></h5>
              <!--h6><%= data[i]._source.category.name %></h6-->
              <h6><i class="fa fa-inr"></i> <%= data[i]._source.price %></h6>

            </div>
          </div>
        </div>
        <% } %>

        <div class="clearfix"> </div>
      </div>
    </div>
  </div>
  <% } %>
</div>
